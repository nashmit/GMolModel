ROUNDS 10 			# MC steps (Acception-rejection steps)
TIMESTEPS 0.006 	# Timesteps to be used with regimens
MDSTEPS  200 	# Number of MD trial steps
MIXMCSTEPS 10 	# Number of MC trials within a mixing round
TEMPERATURE  300 	# Temperature for constant temperature simulations
FFSCALE AMBER 		# Force field scale factors
GBSA 0.0 		# GBSA scale factor
FIXMAN_POTENTIAL FALSE 	# Use Fixman potential 
FIXMAN_TORQUE FALSE 		# Use Fixman torque
VISUAL TRUE  			# Use the visualizer
#VISUAL FALSE  			# Use the visualizer
WRITEPDBS FALSE	 		# Write pdbs
GEOMETRY FALSE  			# Calculate geometric features
DIHEDRAL 1 0 2 3
REPRODUCIBLE TRUE


